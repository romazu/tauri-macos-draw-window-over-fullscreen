{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "frontendDist": "../src"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "tauri-macos-draw-window-over-fullscreen",
  "version": "0.0.0",
  "identifier": "com.tauri-macos-draw-window-over-fullscreen.dev",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "tauri-macos-draw-window-over-fullscreen",
        "width": 200,
        "height": 200,
        "center": true,
        "alwaysOnTop": true,
        "visibleOnAllWorkspaces": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}