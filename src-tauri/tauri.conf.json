{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "tauri-macos-draw-window-over-fullscreen",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "windows": [
      {
        "title": "tauri-macos-draw-window-over-fullscreen",
        "width": 200,
        "height": 200,
        "center": true,
        "alwaysOnTop": true
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri-macos-draw-window-over-fullscreen.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}